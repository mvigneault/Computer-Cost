<html>
<head>
<title>Browser</title>
</head>
<body>
<script type="text/javascript">
 function myFunction() { 
     if((navigator.userAgent.indexOf("Opera") || navigator.userAgent.indexOf('OPR')) != -1 ) 
    {
        alert('Opera');
    }
    else if(navigator.userAgent.indexOf("Chrome") != -1 )
    {
        windows.location.replace("Documents/workspace/computer/bin/computers/new browser/");
    }
    else if(navigator.userAgent.indexOf("Safari") != -1)
    {
        windows.location.replace("Documents/HTML_applet.html");
    }
    else if(navigator.userAgent.indexOf("Firefox") != -1 ) 
    {
         windows.location.replace("Documents/workspace/computer/bin/computers/runapplet.html");
    }
    else if((navigator.userAgent.indexOf("MSIE") != -1 ) || (!!document.documentMode == true )) //IF IE > 10
    {
      alert('IE'); 
    }  
    else 
    {
       alert('unknown');
    }
    }
    
    myFunction()
    
    </script>

    </body>
    </html>
